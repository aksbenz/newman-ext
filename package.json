{
    "name": "newman-ext",
    "version": "1.0.2",
    "description": "Extensions to newman",
    "main": "newman-ext.js",
    "directories": {
        "test": "tests"
    },
    "bin": {
        "newman-ext": "newman-ext.js"
    },
    "scripts": {
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aksbenz/newman-ext.git"
    },
    "jest": {
        "testMatch": [
            "**/tests/**/*.js?(x)",
            "**/?(*.)(spec|test).js?(x)"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            ".*/misc\\.test\\.js",
            ".*/parallel\\.test\\.js"
        ],
        "verbose": true
    },
    "keywords": [
        "newman",
        "postman"
    ],
    "author": "aksbenz",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/aksbenz/newman-ext/issues"
    },
    "homepage": "https://github.com/aksbenz/newman-ext#readme",
    "dependencies": {
        "commander": "^2.11.0",
        "cucumber-tag-expressions": "^1.1.1",
        "lodash": "^4.17.4",
        "newman": "^3.9.4",
        "postman-collection": "^2.1.3",
        "tmp": "0.0.33",
        "uuid": "^3.2.1"
    },
    "devDependencies": {
        "chai": "^4.1.1",
        "jest": "^20.0.4",
        "rewire": "^2.5.2"
    }
}
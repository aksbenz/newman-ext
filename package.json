  },
  "homepage": "https://github.com/aksbenz/newman-ext#readme",
  "dependencies": {
    "commander": "^2.11.0",
    "lodash": "^4.17.4",
    "newman": "^3.8.0",
    "postman-collection": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "jest": "^20.0.4",
    "rewire": "^2.5.2"
